{
    "manifest_version": 2,
    "name": "Professionalize",
    "version": "0.0.1",
    "browser_action": {
        "default_popup": "popup.html",
        "default_icon": "img/icon16.png"
    },
    "permissions": [
        "webRequest",
        "contextMenus",
        "*://*/*"
    ],
    "icons": {
        "16": "img/icon16.png",
        "48": "img/icon48.png",
        "128": "img/icon128.png"
    }
}
